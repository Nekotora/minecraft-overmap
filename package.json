{
  "name": "minecraft-overmap",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn dev:start",
    "dev:start": "node_modules/.bin/nodemon dist/entrypoints/Server.js",
    "start": "node dist/entrypoints/Server.js",
    "watch": "node_modules/.bin/tsc -w",
    "ui:build": "cd ui && yarn && yarn build",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "clean": "node_modules/.bin/del 'dist/*' '!.gitkeep'",
    "build": "node_modules/.bin/tsc"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "delay": "1000",
    "ignore": [
      ".git",
      "node_modules",
      "package.json"
    ],
    "execMap": {
      "": "node --inspect=0",
      "js": "node --inspect=0"
    },
    "watch": [
      "dist"
    ]
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/es6-shim": "^0.31.37",
    "@types/express": "^4.16.0",
    "@types/multer": "^1.3.7",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "~1.18.2",
    "chalk": "^2.4.1",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.9",
    "del": "^3.0.0",
    "express": "~4.15.5",
    "gulp": "^3.9.1",
    "gulp-vueify": "^0.0.3",
    "http": "^0.0.0",
    "http-proxy": "^1.17.0",
    "http-proxy-middleware": "^0.18.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.5.3",
    "nodemon": "^1.18.2",
    "pug": "2.0.0-beta11",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.7",
    "sequelize": "^4.38.0",
    "sequelize-typescript": "^0.6.6-beta.1",
    "serve-favicon": "~2.4.5",
    "typescript": "^2.9.2",
    "vueify-insert-css": "^1.0.0"
  }
}
